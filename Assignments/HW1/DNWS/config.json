{
    "DocumentRoot": ".",
    "Port": "8080",
    //Fix max and min size of threadPool//
    //guided by 600611001//
    "SetMaxThreads" : "20",
    "SetMinThreads" : "5",
    "Plugins" : [
        {
            "Path" : "stat",
            "Class" : "DNWS.StatPlugin",
            "Preprocessing" : "true",
            "Postprocessing" : "false" ,
            "Singpleton" : "false"
        },
        {
            "Path" : "ox",
            "Class" : "DNWS.OXPlugin",
            "Preprocessing" : "false",
            "Postprocessing" :"false" 
        },
        {
          //create a new plugin for the client
          "Path": "ClientInfo",
          "Class" :  "DNWS.ClientPlugin",
          "Preprocessing": "false",
          "Postprocessing": "false"
        }
    ]
}